    "@mui/styled-engine-sc": "^5.12.0",
    "@react-spring/web": "^9.7.3",
    "chakra-react-select": "^4.6.0",
    "framer-motion": "^10.12.4",
    "import": "^0.0.6",
    "next": "^13.3.1",
    "next-pwa": "^5.6.0",
    "next-seo": "^6.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
